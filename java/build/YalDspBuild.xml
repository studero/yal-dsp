<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="CreateYalDspJar" name="Create Jar for YalDsp">
    <target name="CreateYalDspGuiJar">
    	<delete file="YalDspGui.jar"/>
        <jar destfile="YalDspGui.jar">
            <manifest>
                <attribute name="Main-Class" value="ch.sulco.yal.dsp.TestGui"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="..\bin"/>
        </jar>
    </target>
    <target name="CreateYalDspJar">
    	<delete file="YalDsp.jar"/>
        <jar destfile="YalDsp.jar">
            <manifest>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="..\bin">
	            <exclude name="ch\sulco\yal\dsp\TestGui.class"/>
	    	</fileset>
        </jar>
    </target>
</project>
